{
  "name": "fpa",
  "paths": {
    "app": "app/",
    "dev": "_site/",
    "dist": "dist/",
    "diazoPrefix": "/++theme++pkg.name.sitetheme/",
    "src": "node_modules/"
  },
  "favicon": {
    "dataFile": "faviconData.json",
    "masterPicture": "favicon.png",
    "iconPath": "assets/ico/",
    "ios": "#ffffff",
    "windows": "#2d89ef",
    "androidBackground": "#ffffff",
    "androidColor": "#ffffff",
    "safariColor": "#5bbad5",
    "revisionKey": "iet7OjeChoz1",
    "html": "_includes/base/head.html",
    "htmlDist": "_includes/base/"
  },
    "replacementPatterns": {
        "server": [
            {
                "match": "../../assets/",
                "replacement": "../assets/"
            },
            {
                "match": "../../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../../dist/scripts/",
                "replacement": "/scripts/"
            },
            {
                "match": "../dist/scripts/",
                "replacement": "/scripts/"
            }
        ],
        "pat": [
            {
                "match": "../assets/",
                "replacement": "assets/"
            },
            {
                "match": "assets/",
                "replacement": "/++theme++fpa.sitetheme/dist/assets/"
            },
            {
                "match": "../styles/",
                "replacement": "/++theme++fpa.sitetheme/dist/styles/"
            },
            {
                "match": "styles/",
                "replacement": "/++theme++fpa.sitetheme/dist/styles/"
            },
            {
                "match": "/styles/",
                "replacement": "/++theme++fpa.sitetheme/dist/styles/"
            },
            {
                "match": "../scripts/",
                "replacement": "/++theme++fpa.sitetheme/scripts/"
            },
            {
                "match": "scripts/",
                "replacement": "/++theme++fpa.sitetheme/scripts/"
            }
        ]
    }
}
