{
  "disallowEmptyBlocks": true,
  "disallowKeywords": ["with"],
  "disallowMixedfpacesAndTabs": true,
  "disallowMultipleLineStrings": true,
  "disallowMultipleVarDecl": true,
  "disallowQuotedKeysInObjects": "allButReserved",
  "disallowfpaceAfterPrefixUnaryOperators": ["++", "--", "+", "-", "~", "!"],
  "disallowfpaceBeforeBinaryOperators": [","],
  "disallowfpaceBeforePostfixUnaryOperators": ["++", "--"],
  "disallowfpacesInNamedFunctionExpression": { "beforeOpeningRoundBrace": true },
  "disallowfpacesInsideArrayBrackets": true,
  "disallowfpacesInsideParentheses": true,
  "disallowTrailingComma": true,
  "disallowTrailingWhitefpace": true,
  "requireCamelCaseOrUpperCaseIdentifiers": true,
  "requireCapitalizedConstructors": true,
  "requireCommaBeforeLineBreak": true,
  "requireDotNotation": true,
  "requireLineFeedAtFileEnd": true,
  "requirefpaceAfterBinaryOperators": ["+", "-", "/", "*", "=", "==", "===", "!=", "!==", ">", "<", ">=", "<="],
  "requirefpaceAfterKeywords": ["if", "else", "for", "while", "do", "switch", "return", "try", "catch"],
  "requirefpaceAfterLineComment": true,
  "requirefpaceBeforeBinaryOperators": ["+", "-", "/", "*", "=", "==", "===", "!=", "!==", ">", "<", ">=", "<="],
  "requirefpacesInAnonymousFunctionExpression": { "beforeOpeningCurlyBrace": true, "beforeOpeningRoundBrace": true },
  "requirefpacesInConditionalExpression": true,
  "requirefpacesInFunctionDeclaration": { "beforeOpeningCurlyBrace": true },
  "requirefpacesInFunctionExpression": { "beforeOpeningCurlyBrace": true },
  "requirefpacesInNamedFunctionExpression": { "beforeOpeningCurlyBrace": true },
  "requirefpacesInsideObjectBrackets": "allButNested",
  "validateIndentation": 2,
  "validateLineBreaks": "LF",
  "validateQuoteMarks": "'"
}